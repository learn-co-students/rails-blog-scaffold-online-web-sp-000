<p id="notice"><%= notice %></p>

<h1>A Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @a_posts.each do |a_post| %>
      <tr>
        <td><%= a_post.title %></td>
        <td><%= a_post.description %></td>
        <td><%= link_to 'Show', a_post %></td>
        <td><%= link_to 'Edit', edit_a_post_path(a_post) %></td>
        <td><%= link_to 'Destroy', a_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New A Post', new_a_post_path %>
